class Solution {
    public int majorityElement(int[] nums) {
        int count=0;
        int curr=0;

        for(int i=0;i<nums.length;i++){
            curr=nums[i];
            for(int j=0;j<nums.length;j++){
                if(curr==nums[j]){
                    count++;
                }
                
            }
            if(count>=nums.length/2+1){
                break;
             }count=0;
        }
        return curr;
    }
}
